	public void setView(String v) {
		switch (v) {
		case "Run Acceptance":
			AppSession.findElementByName("Run Acceptance_wf").click();
			break;
		case "Legacy":
			AppSession.findElementByName("Legacy_wf").click();
			break;
		case "MDS":
			AppSession.findElementByName("MDS_wf").click();
			break;
		case "Mapping":
			AppSession.findElementByName("Mapping_wf").click();
			break;
		case "DVP":
			AppSession.findElementByName("DVP_wf").click();
			break;
		case "INS":
			AppSession.findElementByName("INS_wf").click();
			break;
		case "Data Processing":
			AppSession.findElementByName("Data Processing_wf").click();
			break;
		case "Safe Passage":
			AppSession.findElementByName("Safe Passage_wf").click();
			break;
		case "New Construction":
			AppSession.findElementByName("New Construction_wf").click();
			break;
		default:
			// code block
		}
	}
}
