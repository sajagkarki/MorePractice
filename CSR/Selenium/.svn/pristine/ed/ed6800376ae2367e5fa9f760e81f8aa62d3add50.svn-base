package userFunctions;

import org.openqa.selenium.WebElement;

import runner.*;


public class OpenRun extends Setup {

	ElementTools et = new ElementTools();
	
	public OpenRun(){
		OpenExistingRun();
	}
	
	private void OpenExistingRun(){
		WebElement openDD = AppSession.findElementByAccessibilityId("comboBox");
		et.dropDownListSelect(openDD, "H10PUL01.fdb");
		//openDD.click();
		//AppSession.findElementByName("H10PUL01.fdb").click();
	}
	
	private void OpenNewRun() {
		WebElement openDD = AppSession.findElementByAccessibilityId("comboBox");
		openDD.click();
		AppSession.findElementByName("H10PUL01.fdb").click();
	}
}
