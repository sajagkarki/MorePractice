export TDW_CONFIG=/usr/local/tomcat/tdw_config
export JAVA_OPTS="$JAVA_OPTS -Djava.security.krb5.conf=/usr/local/tomcat/conf/krb5.conf -Djava.security.auth.login.config=/usr/local/tomcat/conf/login.conf -Dlogin.config.url.1=file:/usr/local/tomcat/conf/login.conf -Dtdw.config=/usr/local/tomcat/tdw_config  -Dfile.encoding=utf-8"
